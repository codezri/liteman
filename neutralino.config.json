{
  "applicationId": "js.neutralino.liteman",
  "port": 0,
  "defaultMode": "window",
  "url": "/",
  "documentRoot": "/resources/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeBlockList": [
    "os.execCommand",
    "filesystem.removeDirectory",
    "filesystem.removeFile"
  ],
  "globalVariables": {},
  "modes": {
    "window": {
      "title": "liteman",
      "width": 800,
      "height": 500,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/icons/appIcon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false
    },
    "browser": {},
    "cloud": {}
  },
  "cli": {
    "binaryName": "liteman",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/liteman-angular/src/assets/neutralino.js",
    "binaryVersion": "4.0.0",
    "clientVersion": "3.0.0"
  }
}
